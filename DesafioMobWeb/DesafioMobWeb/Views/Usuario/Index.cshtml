@model IEnumerable<DesafioMobWeb.Models.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Novo cadastro", "Criar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApelidoUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SenhaUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConfirmarSenhaUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TelefoneUsuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomeUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApelidoUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SenhaUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConfirmarSenhaUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TelefoneUsuario)
        </td>
        <td>
            @Html.ActionLink("Edit", "Editar", new { id=item.UsuarioID }) |
            @Html.ActionLink("Details", "Detalhes", new { id=item.UsuarioID }) |
            
        </td>
    </tr>
}

</table>
